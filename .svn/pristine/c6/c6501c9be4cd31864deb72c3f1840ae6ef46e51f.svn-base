package com.visual.lexML.validations;



/**
 * @author icaroafonso
 *
 */
public class ValidationException extends Exception{
	 
	
		
	private static final long serialVersionUID = 4201291718398753976L;
		private static final String MSG_DESCONHECIDA = "Mensagem desconhecida";

	    public ValidationException() {
	        super(MSG_DESCONHECIDA);
	    }

	    public ValidationException(String message) {
	        super(message);
	    }

	    public ValidationException(Throwable t) {
	        super(t);
	    }

	    public ValidationException(String mensagem, Throwable t) {
	        super(mensagem, t);
	    }

	  
}
